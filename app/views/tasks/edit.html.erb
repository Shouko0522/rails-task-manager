<body>
  <h1>Edit</h1>
  <%= form_for(@task) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :details %>
  <%= f.text_field :details %>

  <%= f.submit %>
<% end %>
  <p><input type="checkbox" name="complete" value="1" checked="checked" ></p>
  <p><%= link_to "Back to list", tasks_path %></p>
</body>
